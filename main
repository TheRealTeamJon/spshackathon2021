import http.client

conn = http.client.HTTPSConnection("power.larc.nasa.gov")
payload = ''
headers = {
  'Cookie': '72433e29e5a9aabda23011a87312c9f6=95caff22feff53d170254d75a7038a05'
}
conn.request("GET", "/api/temporal/daily/point?parameters=T2M&community=RE&longitude=-105.0651&latitude=39.7686&start=20210101&end=20210331&format=JSON", payload, headers)
res = conn.getresponse()
data = res.read()
print(data.decode("utf-8"))
